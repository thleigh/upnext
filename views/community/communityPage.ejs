<div id="topCommunity">

    <div class="subHeaderContainer">
        <h1 class="subHeader">community</h1>
    </div>

    <div id="upload">
        <h1>upload your own:</h1>
        
        <form enctype="multipart/form-data" action="/community" method="POST">
            <!-- <input type="text" name="name" class="comName" placeholder="Title:">
            <br>
            <br> -->
            <input type="file" name="myFile">
            <br>
            <br>
            <input type="submit" class="btn btn-primary">
        </form>
    </div>

</div>
    

<div class="comGrid">
<% if (myPics.length) { %> 
        <% myPics.forEach(p => { %>
            <div id="comImage">
                <img id="comPost" src="<%=p.url%>" alt="picture">
            </div>
        <% }) %>
    <% } else { %>
        <h2>sorry, Nobody posted any pics</h2>
    <% } %> 
</div> 