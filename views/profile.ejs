<h2>Profile Page</h2>

<h3>Welcome to your PROFILE</h3>


<div class="grid">
    <% sneaker.forEach(fav => { %> 
        <div id="shoeResults">
                <img class="shoeImg" src="<%= fav.thumbnail %>" alt="<%= s.shoeName%>">
                <input  hidden type="text" name="id" value="<%= s.styleID%>">
        </div>
    <% }) %> 
</div>    