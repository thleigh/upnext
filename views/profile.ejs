<h2>Profile Page</h2>

<h3>Welcome to your PROFILE</h3>
  
<% sneaker.forEach( fav => { %>
    <div>
        <h2>
            <%= fav %> 
        </h2>
    </div>
    <form method="POST" action="/sneakers?_method=DELETE">
        <input hidden type="text" name="name" value="<%=fav%>">
        <button type="submit">Remove</button>        
    </form>
<% }) %> 