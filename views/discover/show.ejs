<h2>Discover</h2>

<!-- Displays the results of the shoe search. -->
    <% products.forEach(s => { %> 
        <div id="productshoeResults">
            <h3 class="shoeName"><%= s.shoeName %></h3>
            <img class="shoeImg" src="<%= s.thumbnail %>" alt="<%= s.shoeName%>">
        </div>
    <% }) %> 